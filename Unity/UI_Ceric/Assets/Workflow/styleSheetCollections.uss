    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    height: 60px;
}

.scrollView {
    left: auto;
    align-items: center;
    justify-content: flex-start;
    align-self: auto;
    -unity-text-align: upper-left;
    top: auto;
}

.vignette {
    background-image: url('project://database/Assets/Art/Bkg/collectionsBkgGradience.png?fileID=2800000&guid=1f97c32c128c20a4fae6796d3721a628&type=3#collectionsBkgGradience');
