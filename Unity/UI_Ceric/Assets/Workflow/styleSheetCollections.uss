    flex-wrap: nowrap;
    border-left-color: rgba(255, 255, 255, 0.2);
    border-right-color: rgba(255, 255, 255, 0.2);
    border-top-color: rgba(255, 255, 255, 0.2);
    border-bottom-color: rgba(255, 255, 255, 0.2);
    flex-grow: 0;
    height: auto;
    flex-shrink: 1;
    width: 500px;
}

.labelTitle {
    margin-left: 6px;
    margin-right: 6px;
    margin-top: 2px;
    margin-bottom: 2px;
    padding-left: 6px;
    padding-right: 6px;
    padding-top: 2px;
    padding-bottom: 2px;
    -unity-font-style: bold;
    color: rgb(255, 255, 255);
    font-size: 36px;
    text-shadow: 2px 2px 0 rgb(108, 18, 32);
    white-space: normal;
    justify-content: flex-end;
    -unity-text-align: middle-center;
    align-items: stretch;
    align-self: center;
    flex-wrap: wrap;
}

.indvRingImg {
}

.labelDescription {
    color: rgb(255, 255, 255);
    font-size: 24px;
    margin-left: 6px;
    margin-right: 6px;
    margin-top: 2px;
    margin-bottom: 2px;
    padding-left: 6px;
    padding-right: 6px;
    padding-top: 2px;
    padding-bottom: 2px;
    align-items: stretch;
    align-self: center;
    flex-wrap: wrap;
    -unity-text-align: middle-center;
    white-space: normal;
    word-spacing: 0;
    -unity-paragraph-spacing: 0;
    letter-spacing: 0;
    text-shadow: 2px 2px 0 rgb(108, 18, 32);
    justify-content: flex-end;
}

.toggleLike {
    align-items: auto;
    justify-content: flex-start;
    align-self: auto;
    margin-left: 3px;
    margin-right: 3px;
    margin-top: 3px;
    margin-bottom: 3px;
    padding-left: 3px;
    padding-right: 3px;
    padding-top: 3px;
    padding-bottom: 3px;
    overflow: visible;
    position: absolute;
    left: auto;
    right: 25px;
    top: 25px;
    width: auto;
    height: auto;
    transform-origin: center;
    scale: 6 6;
    opacity: 0.35;
}

.buttonCustomise {
    border-top-left-radius: 24px;
    border-bottom-left-radius: 24px;
    border-top-right-radius: 24px;
    border-bottom-right-radius: 24px;
    margin-left: 6px;
    margin-right: 6px;
    margin-top: 12px;
    margin-bottom: 12px;
    padding-left: 6px;
    padding-right: 6px;
    padding-top: 12px;
    padding-bottom: 12px;
    font-size: 22px;
    -unity-text-align: middle-center;
    white-space: nowrap;
    border-left-color: rgb(200, 18, 48);
    border-right-color: rgb(200, 18, 48);
    border-top-color: rgb(200, 18, 48);
    border-bottom-color: rgb(200, 18, 48);
    width: 100%;
    color: rgb(255, 255, 255);
    -unity-font: url('project://database/Assets/UI%20Toolkit/UnityThemes/UnityDefaultRuntimeTheme.tss?fileID=2230732570650464555&guid=6f95556c412239c4c8cbcb90fa0a041a&type=3#NotInter-Regular');
