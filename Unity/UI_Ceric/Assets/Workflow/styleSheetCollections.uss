.indvRingContainer {
    margin-left: 36px;
    margin-right: 36px;
    margin-top: 36px;
    margin-bottom: 36px;
    padding-left: 12px;
    padding-right: 12px;
    padding-top: 12px;
    padding-bottom: 12px;
    align-self: auto;
    justify-content: flex-start;
    align-items: center;
    -unity-text-outline-width: 0;
    border-left-width: 5px;
    border-right-width: 5px;
    border-top-width: 5px;
    border-bottom-width: 5px;
    border-top-left-radius: 48px;
    border-bottom-left-radius: 48px;
    border-top-right-radius: 48px;
    border-bottom-right-radius: 48px;
    background-color: rgba(35, 35, 35, 0.59);
    -unity-text-outline-color: rgba(0, 0, 0, 0);
    translate: 0 0;
    scale: 1 1;
    top: auto;
    background-image: url('project://database/Assets/Art/Bkg/indvRingContainerBkgGradience.png?fileID=21300000&guid=aef0103981580794aa164627d748ea52&type=3#indvRingContainerBkgGradience');
